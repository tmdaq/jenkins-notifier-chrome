
{
    "manifest_version":2,//版本号，由google指定为2
    "name": "jenkins-notifier-chrome",//插件名称
    "version":"1.0",//插件版本
    "description":"Jenkins Chrome Plugin",//插件描述
    "icons":{//插件图标
        "128":"img/logo.png",
        "48":"img/logo.png",
        "16":"img/logo.png"
    },
    "browser_action":{
        "default_icon":"img/logo.png",//插件图标
        "default_popup":"popup.html"//点击图标后弹出的html互动文件
    }
    
}